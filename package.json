{
	"name": "next-webcode",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build:internal": "next build",
		"build": "pnpm dlx rimraf .next && next build",
		"start": "next start",
		"kill": "pnpm dlx kill-port 3000 3001 3002",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "pnpm dlx prettier --write .",
		"analyze": "cross-env ANALYZE=true next build",
		"clean:install": "rm -rf .next && pnpm store prune && pnpm install --force",
		"notion:verify": "node scripts/verify-notion-connection.js",
		"notion:list": "node scripts/list-notion-databases.js",
		"notion:search": "node scripts/search-notion-database.js",
		"notion:content": "node scripts/list-database-content.js",
		"notion:pages": "node scripts/show-page-content.js",
		"notion:publish": "node scripts/publish-page.js"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.1",
		"@notionhq/client": "^5.1.0",
		"@radix-ui/react-accordion": "^1.2.12",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slot": "^1.2.3",
		"@react-email/render": "^1.3.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"framer-motion": "^12.23.12",
		"highlight.js": "^11.11.1",
