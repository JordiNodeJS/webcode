		"jspdf-autotable": "^5.0.2",
		"lucide-react": "^0.542.0",
		"motion": "^12.23.12",
		"next": "16.0.0",
		"next-themes": "^0.4.6",
		"next-view-transitions": "^0.3.4",
		"notion-to-md": "^3.1.9",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"react-hook-form": "^7.62.0",
		"react-icons": "^5.5.0",
		"react-markdown": "^10.1.0",
		"rehype-highlight": "^7.0.2",
		"rehype-raw": "^7.0.0",
		"rehype-sanitize": "^6.0.0",
		"resend": "^6.1.0",
